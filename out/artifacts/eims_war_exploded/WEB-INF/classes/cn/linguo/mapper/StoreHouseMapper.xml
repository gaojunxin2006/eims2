<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.linguo.mapper.StoreHouseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.linguo.entity.StoreHouse">
        <id column="sh_id" property="shId" />
        <result column="sh_name" property="shName" />
        <result column="sh_responsible" property="shResponsible" />
        <result column="sh_phone" property="shPhone" />
        <result column="sh_address" property="shAddress" />
        <result column="sh_type" property="shType" />
        <result column="sh_remark" property="shRemark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sh_id, sh_name, sh_responsible, sh_phone, sh_address, sh_type, sh_remark
    </sql>

</mapper>
